{
    "name": "interviewai",
    "type": "module",
    "private": true,
    "scripts": {
        "install": "npm --prefix frontend ci",
        "build": "npm --prefix frontend run build",
        "dev": "concurrently \"npm --prefix frontend run dev\" \"npm --prefix backend run dev\"",
        "start": "npm --prefix backend start",
        "bundle:api": "npx esbuild backend/src/serverless.ts --bundle --platform=node --packages=external --outfile=api/index.js --format=esm"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "esbuild": "^0.25.0"
    },
    "dependencies": {
        "axios": "^1.13.5",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.3",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-validator": "^7.3.1",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.10.1",
        "multer": "^2.0.2",
        "openai": "^4.85.4",
        "pdf-parse-fork": "^1.2.0",
        "serverless-http": "^4.0.0"
    }
}