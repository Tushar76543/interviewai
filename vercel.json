{
    "version": 2,
    "buildCommand": "npm --prefix frontend install && npm --prefix frontend run build",
    "outputDirectory": "frontend/dist",
    "installCommand": "npm install",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index.js"
        }
    ],
    "functions": {
        "api/index.js": {
            "maxDuration": 30
        }
    }
}